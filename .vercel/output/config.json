{
  "version": 3,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "^/api(/.*)?$",
      "status": 404
    },
    {
      "src": "/.*",
      "dest": "render"
    },
    {
      "handle": "error"
    },
    {
      "status": 404,
      "src": "^(?!/api).*$",
      "dest": "/404.html"
    },
    {
      "handle": "miss"
    },
    {
      "src": "^/api/(.+)(?:\\.(?:js))$",
      "dest": "/api/$1",
      "check": true
    }
  ],
  "framework": {
    "version": "2.1.5"
  },
  "crons": []
}
